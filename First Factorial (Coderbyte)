int FirstFactorial(int num) {
  
  // code goes here 
  if(num>=1)
  {
    return num * FirstFactorial(num-1);
  }
  else
  {
    return 1;
  }
  
}

int main(void) { 
  int result, n;
  printf("Enter a number to find factorial");
  scanf("%d", &n);
  result = FirstFactorial(n);
  printf("%d", result);
  return 0;
    
}
